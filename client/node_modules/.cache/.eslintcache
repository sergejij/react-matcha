rev();\n    }\n    if (e.keyCode === 39 && hasNext) {\n      findNext();\n    }\n  };\n\n  React.useEffect(() => {\n    document.body.addEventListener('keydown', handleKeyDown);\n    return () => document.body.removeEventListener('keydown', handleKeyDown);\n  }, []);\n\n  if (!src) {\n    return null;\n  }\n\n  return (\n    <div>\n      <ModalOverlay onClick={closeModal} />\n      <Modal isOpen={!!src}>\n        <ModalBody>\n          <a href=\"#\" className=\"modal-close\" onClick={closeModal} onKeyDown={handleKeyDown}>&times;</a>\n          {hasPrev && <a href=\"#\" className=\"modal-prev\" onClick={findPrev} onKeyDown={handleKeyDown}>&lsaquo;</a>}\n          {hasNext && <a href=\"#\" className=\"modal-next\" onClick={findNext} onKeyDown={handleKeyDown}>&rsaquo;</a>}\n          <img src={src} alt=\"asf\" />\n        </ModalBody>\n      </Modal>\n    </div>\n  );\n};\n","C:\\Users\\79050\\WebstormProjects\\react-matcha\\client\\src\\components\\Button\\styled.jsx",[],"C:\\Users\\79050\\WebstormProjects\\react-matcha\\client\\src\\pages\\Chats\\ChatsUser\\styled.jsx",[],"C:\\Users\\79050\\WebstormProjects\\react-matcha\\client\\src\\pages\\Profile\\ProfilePhotos\\Slider\\styled.jsx",[],{"ruleId":"232","severity":1,"message":"233","line":12,"column":1,"nodeType":"234","endLine":31,"endColumn":3},{"ruleId":"232","severity":1,"message":"233","line":6,"column":1,"nodeType":"234","endLine":10,"endColumn":3},{"ruleId":"232","severity":1,"message":"233","line":11,"column":1,"nodeType":"234","endLine":29,"endColumn":3},{"ruleId":"232","severity":1,"message":"233","line":8,"column":1,"nodeType":"234","endLine":33,"endColumn":3},{"ruleId":"235","severity":1,"message":"236","line":27,"column":9,"nodeType":"237","endLine":27,"endColumn":81},{"ruleId":"235","severity":1,"message":"236","line":66,"column":9,"nodeType":"237","endLine":66,"endColumn":78},{"ruleId":"232","severity":1,"message":"233","line":108,"column":1,"nodeType":"234","endLine":152,"endColumn":3},{"ruleId":"232"